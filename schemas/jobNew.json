{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": ["title", "companyHandle"],
  "additionalProperties": false,
  "properties": {
    "title": {
      "type": "string",
      "default": "",
      "title": "The title Schema",
      "examples": ["CEO"]
    },
    "salary": {
      "type": "integer",
      "default": 0,
      "title": "The salary Schema",
      "examples": [100000]
    },
    "equity": {
      "type": "number",
      "default": 0.0,
      "title": "The equity Schema",
      "examples": [0.27]
    },
    "companyHandle": {
      "type": "string",
      "default": "",
      "title": "The companyHandle Schema",
      "examples": ["comphandle"]
    }
  },
  "examples": [
    {
      "title": "CEO",
      "salary": 100000,
      "equity": 0.27,
      "companyHandle": "comphandle"
    }
  ]
}
